{
    "name": "cideator/ciadmin",
    "description": "The base package for Laravel Admin Starter Kit",
    "type": "ciadmin-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Farhan Wazir",
            "email": "farhan.wazir@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework" : "5.*",
        "riverskies/laravel-mobile-detect": "^1.3",
        "laracasts/flash": "^3.0",
        "ixudra/curl": "^6.16",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "spatie/laravel-permission": "^2.17"
    },
    "autoload": {
        "files": [
            "src/Admin/base/helpers.php",
            "src/Admin/template/src/Template/Base/helpers.php"
        ],
        "psr-4": {
            "CIAdmin\\": "src/Admin",
            "CIAdmin\\Base\\": "src/Admin/base",
            "CIAdmin\\Foundation\\": "src/Admin/foundation/src/Foundation",
            "CIAdmin\\Support\\": "src/Admin/support/src/Support",
            "CIAdmin\\Template\\": "src/Admin/template/src/Template"
        }
    }
}
